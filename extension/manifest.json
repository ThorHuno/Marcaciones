{
    "manifest_version": 2,
    "name": "Marcaciones",
    "version": "1.0",
    "description": "Extensi√≥n de Chrome para marcar entrada/salida laboral",
    "icons": {
        "16": "iconos/16-icono.png",
        "128": "iconos/128-icono.png"
    },
    "background": {
        "scripts": [
            "axios.min.js",
            "background.js",
            "socket.io.js"
        ],
        "persistent": false
    },
    "permissions": [
        "identity",
        "identity.email",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Axxis Systems"
    }
}